<p id="notice"><%= notice %></p>

<h1>Listing Records</h1>

<table>
  <thead>
    <tr>
      <th>Collar</th>
      <th>Latitude</th>
      <th>Longitude</th>
      <th>Breathing freq</th>
      <th>Heart freq</th>
      <th>Systolic</th>
      <th>Diastolic</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @records.each do |record| %>
      <tr>
        <td><%= record.collar %></td>
        <td><%= record.latitude %></td>
        <td><%= record.longitude %></td>
        <td><%= record.breathing_freq %></td>
        <td><%= record.heart_freq %></td>
        <td><%= record.systolic %></td>
        <td><%= record.diastolic %></td>
        <td><%= link_to 'Show', record %></td>
        <td><%= link_to 'Edit', edit_record_path(record) %></td>
        <td><%= link_to 'Destroy', record, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Record', new_record_path %>
